{
  "version": "0.0.1",
  "description": "The Elrond blockchain on metamask",
  "proposedName": "Multiversx",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kyraview/elrondSnap.git"
  },
  "source": {
    "shasum": "Sd5R9Z1OoPuyEm1K19MmTh8qt4l8ecxFAQDW3pl0I3I=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "packageName": "multiversx",
        "registry": "https://registry.npmjs.org/",
        "iconPath": "images/icon.svg"
      }
    }
  },
  "initialPermissions": {
    "snap_confirm": {},
    "endowment:network-access": {},
    "snap_manageState": {},
    "endowment:long-running": {},
    "snap_notify": {},
    "snap_getBip44Entropy": [
      {
        "coinType": 508
      }
    ],
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "cron",
            "params": {
              "go": "yes"
            }
          }
        }
      ]
    }
  },
  "manifestVersion": "0.1"
}
